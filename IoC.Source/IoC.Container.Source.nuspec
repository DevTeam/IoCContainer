<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="3.3.0">
    <id>IoC.Container.Source</id>
    <authors>NikolayP</authors>
    <version>$version$</version>
    <copyright>Copyright (c) 2019 Nikolay Pianikov</copyright>
    <icon>common\icon.png</icon>
    <projectUrl>https://github.com/DevTeam/IoCContainer/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Simple, powerful and fast expressions based Inversion of Control container for .NET as embedding-in-code package.</description>
    <tags>ioc container di</tags>
    <contentFiles>
      <files include="cs/any/*.cs" buildAction="Compile" />
    </contentFiles>
    <dependencies>
        <group targetFramework="net40"/>
    </dependencies>
  </metadata>
  <files>
    <file src="icon.png" target="common" />
    <file src="*.cs" target="contentFiles\cs\any" />
    <file src="IoC.Container.Source.targets" target="build\net40" />
    <file src="empty.empty" target="lib\net40" />
  </files>
</package>